<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zj.mybatis.mapper.ClassesMapper">
	
	<resultMap type="Classes" id="classesResultMap">
		<id column="cid" property="id" jdbcType="INTEGER"></id>
		<id column="cname" property="name" jdbcType="VARCHAR"></id>
	</resultMap>
	
	<select id="selectClassesById" parameterType="int" resultMap="classesResultMap">
		select * from Classes where cid = #{id}
	</select>
</mapper>